<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <!-- for-mobile-apps -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Auditing Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--// Meta tag Keywords -->
    <!-- css files -->
    <!-- //for-mobile-apps -->
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Content/AdminLTE/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/AdminLTE/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Content/AdminLTE/ionicons.min.css">
    <!-- jvectormap -->
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/AdminLTE/AdminLTE.min.css">
    <link rel="stylesheet" href="~/Content/AdminLTE/skin-blue.min.css">
    <!-- ~/Scripts/Home -->
    <!-- //~/Scripts/Home -->
    <script type="text/javascript" src="~/Scripts/Home/jquery-2.1.4.min.js"></script>
    <link href="~/Content/Home/style.css" rel="stylesheet" type="text/css" media="all" />

    <!-- font-awesome icons -->
    <!-- //font-awesome icons -->
    <link rel="stylesheet" href="~/Content/Home/flexslider.css" type="text/css" media="screen" property="" />
