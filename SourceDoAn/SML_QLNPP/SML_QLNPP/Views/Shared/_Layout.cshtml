    <script src="~/scripts/AdminLTE/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/scripts/AdminLTE/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/scripts/AdminLTE/adminlte.min.js"></script>

    @RenderSection("scripts", required: false)


